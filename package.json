{
  "name": "react-webpack-single-page-app",
  "version": "1.3.0",
  "description": "A combination of a few existing React/Webpack boilerplates to make cooking up single page apps easy.",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run destroy && NODE_ENV=development npm run start",
    "build": "BROWSERSLIST_CONFIG=./browserslist npm run clean && NODE_ENV=production webpack --config webpack.config.prod.js",
    "destroy": "trash ./public/**/*build*",
    "lint": "eslint ./src"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewGibson27/react-webpack-single-page-app"
  },
  "author": "Andrew Gibson <atgibson27@gmail.com>",
  "bugs": {
    "url": "https://github.com/AndrewGibson27/react-webpack-single-page-app/issues"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.0",
    "cssnano": "^3.7.5",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^0.9.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "trash-cli": "^1.3.0",
    "uglify-save-license": "^0.4.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "webfontloader": "^1.6.24"
  }
}
